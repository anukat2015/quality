#--- Timeliness Dimension ------------------------------------------------------------------------------------------------- 

_:CurrencyOfDataset a lmi:LuzzuMetricJavaImplementation ;
	rdfs:label "Currency of Dataset" ;
	lmi:javaPackageName "eu.diachron.qualitymetrics.contextual.timeliness.CurrencyOfDataset" .
	
_:FreshnessOfDataset a lmi:LuzzuMetricJavaImplementation ;
	rdfs:label "Dereferenceability of Forward-links" ;
	lmi:javaPackageName "eu.diachron.qualitymetrics.contextual.timeliness.FreshnessOfDataset" .
	
